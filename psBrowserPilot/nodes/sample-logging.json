{
  "id": "log_message",
  "label": "Logging: Write-Information",
  "category": "Samples",
  "description": "Writes an information record with the provided message.",
  "inputs": ["Message"],
  "outputs": [],
  "constants": [
    {
      "key": "tag",
      "default": "psBrowserPilot"
    }
  ],
  "script": "param($Message)\nWrite-Information -MessageData $Message -Tags '{{config.tag}}'"
}
